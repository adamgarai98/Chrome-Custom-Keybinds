{
    "manifest_version": 3,
    "name": "Shortcut Launcher",
    "version": "1.0",
    "description": "Quickly launch your favorite websites with custom keyboard shortcuts.",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "storage"
    ],
    "action": {
        "default_icon": {
            "128": "icon128.png"
        },
        "default_title": "Keybind Launcher",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+Z"
            }
        }
    }
}